{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    // "eslint:recommended",
    "plugin:react/recommended",
    "airbnb",
    "prettier"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module",
    "parser": "babel-eslint"
  },
  "plugins": ["react", "prettier"],
  "rules": {
    "semi": [2, "always"],
    "quotes": ["error", "single"]
  }
}
